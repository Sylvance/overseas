<xsd:schema xmlns:ups="http://www.ups.com/XMLSchema" xmlns:xsd="http://www.w3.org/2001/XMLSchema" elementFormDefault="unqualified" version="201707">	<xsd:element name="LabelRecoveryResponse">		<xsd:complexType>			<xsd:sequence>				<xsd:element ref="Response"/>				<xsd:element name="ShipmentIdentificationNumber" type="xsd:string" minOccurs="0"/>				<xsd:element name="CODTurnInPage" type="CODTurnInPageType" minOccurs="0"/>				<xsd:element name="Form" type="FormType" minOccurs="0"/>				<xsd:element name="HighValueReport" type="HighValueReportType" minOccurs="0"/>				<xsd:choice minOccurs="0" maxOccurs="unbounded">					<xsd:element name="LabelResults" type="LabelResultsType"/>					<xsd:element name="TrackingCandidate" type="TrackingCandidateType"/>				</xsd:choice>			</xsd:sequence>		</xsd:complexType>	</xsd:element>	<xsd:complexType name="LabelResultsType">		<xsd:sequence>			<xsd:element name="TrackingNumber" type="xsd:string"/>			<xsd:element name="LabelImage" type="LabelImageType"/>			<xsd:element name="Receipt" type="ReceiptType" minOccurs="0"/>			<xsd:element name="Form" type="FormType" minOccurs="0"/>			<xsd:element name="MailInnovationsTrackingNumber" type="xsd:string" minOccurs="0"/>			<xsd:element name="MailInnovationsLabelImage" type="LabelImageType" minOccurs="0"/>		</xsd:sequence>	</xsd:complexType>	<xsd:complexType name="LabelImageType">		<xsd:sequence>			<xsd:element name="LabelImageFormat" type="CodeType"/>			<xsd:element name="GraphicImage" type="xsd:string"/>			<xsd:element name="HTMLImage" type="xsd:string" minOccurs="0"/>			<xsd:element name="PDF417" type="xsd:string" minOccurs="0"/>			<xsd:element name="InternationalSignatureGraphicImage" type="xsd:string" minOccurs="0"/>			<xsd:element name="URL" type="xsd:string" minOccurs="0"/>		</xsd:sequence>	</xsd:complexType>	<xsd:complexType name="ReceiptType">		<xsd:sequence>			<xsd:element name="HTMLImage" type="xsd:string" minOccurs="0"/>			<xsd:element name="Image" type="ImageType" minOccurs="0"/>		</xsd:sequence>	</xsd:complexType>	<xsd:complexType name="ImageType">		<xsd:sequence>			<xsd:element name="ImageFormat" type="CodeType" minOccurs="0"/>			<xsd:element name="GraphicImage" type="xsd:string" minOccurs="0"/>		</xsd:sequence>	</xsd:complexType>	<xsd:complexType name="CodeType">		<xsd:sequence>			<xsd:element name="Code" type="xsd:string"/>		</xsd:sequence>	</xsd:complexType>	<xsd:complexType name="TrackingCandidateType">		<xsd:sequence>			<xsd:element name="TrackingNumber" type="xsd:string"/>			<xsd:element name="DestinationPostalCode" type="xsd:string" minOccurs="0"/>			<xsd:element name="DestinationCountryCode" type="xsd:string" minOccurs="0"/>			<xsd:element name="PickupDateRange" type="PickupDateRangeType" minOccurs="0"/>		</xsd:sequence>	</xsd:complexType>	<xsd:complexType name="PickupDateRangeType">		<xsd:sequence>			<xsd:element name="BeginDate" type="xsd:string"/>			<xsd:element name="EndDate" type="xsd:string"/>		</xsd:sequence>	</xsd:complexType>	<xsd:element name="Response">		<xsd:complexType>			<xsd:sequence>				<xsd:element ref="TransactionReference" minOccurs="0"/>				<xsd:element name="ResponseStatusCode" type="xsd:string"/>				<xsd:element name="ResponseStatusDescription" type="xsd:string" minOccurs="0"/>				<xsd:element ref="Error" minOccurs="0" maxOccurs="unbounded"/>			</xsd:sequence>		</xsd:complexType>	</xsd:element>	<xsd:element name="TransactionReference">		<xsd:complexType>			<xsd:sequence>				<xsd:element name="CustomerContext" type="xsd:string" minOccurs="0"/>				<xsd:element name="XpciVersion" type="xsd:string" minOccurs="0"/>			</xsd:sequence>		</xsd:complexType>	</xsd:element>	<xsd:element name="Error">		<xsd:complexType>			<xsd:sequence>				<xsd:element name="ErrorSeverity" type="xsd:string"/>				<xsd:element name="ErrorCode" type="xsd:string"/>				<xsd:element name="ErrorDescription" type="xsd:string" minOccurs="0"/>				<xsd:element name="MinimumRetrySeconds" type="xsd:string" minOccurs="0"/>				<xsd:element ref="ErrorLocation" minOccurs="0" maxOccurs="unbounded"/>				<xsd:element name="ErrorDigest" type="xsd:string" minOccurs="0" maxOccurs="unbounded"/>			</xsd:sequence>		</xsd:complexType>	</xsd:element>	<xsd:element name="ErrorLocation">		<xsd:complexType>			<xsd:sequence>				<xsd:element name="ErrorLocationElementName" type="xsd:string" minOccurs="0"/>				<xsd:element name="ErrorLocationElementReference" type="xsd:string" minOccurs="0" ups:usage="notused"/>				<xsd:element name="ErrorLocationAttributeName" type="xsd:string" minOccurs="0"/>			</xsd:sequence>		</xsd:complexType>	</xsd:element>	<xsd:complexType name="CODTurnInPageType">		<xsd:sequence>			<xsd:element name="Image" type="ImageType2"/>		</xsd:sequence>	</xsd:complexType>	<xsd:complexType name="ImageType2">		<xsd:sequence>			<xsd:element name="ImageFormat" type="LabelImageFormatType2" minOccurs="0"/>			<xsd:element name="GraphicImage" type="xsd:string" minOccurs="0"/>		</xsd:sequence>	</xsd:complexType>	<xsd:complexType name="LabelImageFormatType2">		<xsd:sequence>			<xsd:element name="Code" type="xsd:string"/>			<xsd:element name="Description" type="xsd:string" minOccurs="0"/>		</xsd:sequence>	</xsd:complexType>	<xsd:complexType name="FormType">		<xsd:sequence>			<xsd:element name="Image" type="FormImageType"/>		</xsd:sequence>	</xsd:complexType>	<xsd:complexType name="FormImageType">		<xsd:sequence>			<xsd:element name="ImageFormat" type="LabelImageFormatType2"/>			<xsd:element name="GraphicImage" type="xsd:string"/>		</xsd:sequence>	</xsd:complexType>	<xsd:complexType name="HighValueReportType">		<xsd:sequence>			<xsd:element name="Image" type="HVRImageType"/>		</xsd:sequence>	</xsd:complexType>	<xsd:complexType name="HVRImageType">		<xsd:sequence>			<xsd:element name="ImageFormat" type="LabelImageFormatType2"/>			<xsd:element name="GraphicImage" type="xsd:string"/>		</xsd:sequence>